{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/api/index.ts"
        },
        {
            "source": "/(.*\\..+)",
            "destination": "/client/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/client/index.html"
        }
    ]
}